<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:application="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd" id="Application_ID" version="6">
  <display-name>Web2SMS-UI_EAR</display-name>
  <module>
    <web>
      <web-uri>Web2sms_UI-main.war</web-uri>
      <context-root>web2sms</context-root>
    </web>
  </module>
  <module>
    <ejb>Web2sms_UI-Utils.jar</ejb>
  </module>
  <module>
    <ejb>AppErrorManager.jar</ejb>
  </module>
  <library-directory>lib</library-directory>
  <env-entry>
    <env-entry-name>java:app/env/basedir</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>web2sms-ui/</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>java:app/env/basedirfiles</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>web2sms-ui/files</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>java:app/env/baseconfigdir</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>web2sms-ui/configuration/</env-entry-value>
  </env-entry>
</application>